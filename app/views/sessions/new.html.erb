<h2>Sign In</h2>
<%= form_for(:session, :url => sessions_path) do |f| %>
  
  
  
  <div class="field">
    <%= f.label :email %>
    <%= f.text_field :email %>
  </div>
  <div class="field">
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>
  <div class="actions">
    <%= f.submit "Sign In" %>
  </div>
<% end %>

