<h2>All Press Releases</h2>

<table>
	<% @press_releases.each do |press_release| %>
	  <tr>
	  	<td><%= image_tag press_release.brand.icon, {:height => 75} %></td>
	    <td><%= render(press_release) %></td>
	    <td><%= render(press_release.user) %></td>
	  </tr>
	<% end %>
</table>

<br />


<%= link_to 'New Press Release', new_press_release_path, :class => "admin new" %>
